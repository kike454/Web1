
<%- include("header", {}) %>
<table>
    <tr>
    <th>Username</th>
    <th>Role</th>
    <th>Last Login</th>
    <th>Delete</th>
    </tr>
    <% Object.keys(usuarios).forEach((nombreUsuario) => { %>
    <tr>
    <td> <%=usuarios[nombreUsuario].username %> </td>
    <td> <%=usuarios[nombreUsuario].role %> </td>
    <td> <%=usuarios[nombreUsuario].last_Login %></td>
    <td>
    <%if(usuarios[nombreUsuario].role !== 'admin'){ %> 
        <form action="/admin/deleteUser" method="post">
          <button name="username" value='<%=usuarios[nombreUsuario].username %>'>delete</button>
        </form> 
    
    <% } %>
    </td>
    </tr>
    <% }) %>
    </table>

<%- include("footer", {}) %>